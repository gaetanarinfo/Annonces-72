<section class="dark-wrapper opaqued parallax" data-parallax="scroll" data-image-src="http://localhost:8000/img/bg/bg3.jpg" data-speed="0.7">
	<div
		id="contact-tabs" role="tabpanel">

		<!-- Nav tabs -->
		<ul class="nav nav-justified icon-tabs" id="nav-tabs" role="tablist">
			<li role="presentation" class="ptb smoothie active">
				<a href="#contact" aria-controls="contact" role="tab" data-toggle="tab">
					<span class="tabtitle heading-font smoothie text-center">Contactez-nous</span>
				</a>
			</li>
		</ul>

		<!-- Tab panes -->
		<div class="tab-content" id="tabs-collapse">
			<div role="tabpanel" class="tab-pane fade in active" id="contact">
				<div id="contact-inner" class="nopadding-lr">
					<div class="contact-section-inner">
						<div class="container">
							<div class="row">
								<div class="col-sm-8 col-sm-offset-2">
									{{ form_start(formContact, {'attr' : {'class' : 'main-contact-form wow animated'}}) }}
										{{ form_widget(formContact.lastname) }}
										{{ form_widget(formContact.email) }}
										{{ form_widget(formContact.subject) }}
										{{ form_widget(formContact.content) }}
										<button class="btn btn-primary mt30 btn-white pull-right" type="submit">Envoyer</button>
									{{ form_end(formContact) }}
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>

		</div>
	</div>
</section>
